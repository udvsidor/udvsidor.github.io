              }
            },
            ".mbr-form": {
              ".mbr-overlay": {
                "opacity": "@formBgOpacity",
                "background": "@formBgColor"
              },
              "input, textarea": {
                "background-color": "@fieldColor",
                "color": "@labelsColor !important",
                "border-color": "@borderColor"
              },
              "& when (@showIcons)": {
                ".iconfont-wrapper": {
